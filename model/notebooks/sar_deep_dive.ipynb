    "\n",
    "data = pd.read_csv('../../data/' + dataset + '/recipe_reviews.csv',encoding = \"ISO-8859-1\",delimiter=\"|\")\n",
    "features = pd.read_csv('../..data/' + dataset + '/recipe_features.csv',encoding = \"ISO-8859-1\",delimiter=\"|\")\n",
    "\n",
    "# data = pd.read_csv('data/recipes_final.csv',encoding = \"ISO-8859-1\",delimiter=\"|\")\n",
    "# features = pd.read_csv('data/recipe_features.csv',encoding = \"ISO-8859-1\",delimiter=\"|\")\n",
    "\n",
